{
  "name": "@drill4j/admin-ui-container",
  "author": "Drill4J",
  "license": "Apache-2.0",
  "description": "Drill4J admin ui root config",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run tailwind:css && webpack serve --port 9000 --env isLocal",
    "lint": "eslint src --ext js,ts,tsx",
    "lint:fix": "eslint src --ext js,ts,tsx --fix",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "build": "npm run tailwind:css && webpack --mode=production",
    "tailwind:css": "tailwind build src/ui/src/tailwind.css -c tailwind.config.js -o src/ui/src/index.css"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && concurrently npm:test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint src --ext js,ts,tsx --fix"
    ]
  },
